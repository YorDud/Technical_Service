<Window x:Class="WpfApp4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp4"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="MainWindow" Height="1080" Width="1920" WindowStartupLocation="CenterScreen" WindowState="Maximized" Loaded="Window_Loaded" Closed="Window_Closed">
    <Window.Resources>
        <local:Lab_RezDataSet x:Key="lab_RezDataSet"/>
        <CollectionViewSource x:Key="test_tableViewSource" Source="{Binding test_table, Source={StaticResource lab_RezDataSet}}"/>
    </Window.Resources>


    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <!-- Левое меню -->
        <StackPanel Background="#f0f0f0" Grid.Column="0">
            <Button Content="🏠 Мониторинг ТО" FontSize="16" Margin="10" Padding="10" Height="45" 
                    HorizontalAlignment="Stretch" Click="ShowMonitoringTO" />

            <Button Content="📈 Мониторинг поломок" FontSize="13" Margin="10" Padding="10" Height="45" 
        HorizontalAlignment="Stretch" Click="ShowMonitoringCrash" />

            <Expander Header="📂 Главное" FontSize="16" Margin="10" Padding="10">
                <StackPanel>

                    <Button Content="Оборудование" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="ShowDevices"/>
                    <Button Content="Наряды" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="NarTObtn"/>
                    <Button Content="Виды ТО" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="TechObslbtn"/>
                    <Button Content="Сотрудники" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="Users_Nalad_Click"/>
                    <Button Content="Документация" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="Show_Jurnal_Docs"/>
                </StackPanel>
            </Expander>

            <Expander Header="📂 Склад" FontSize="14" Margin="10" Padding="10">
                <StackPanel>

                    <Button Content="Склад" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="Sklad_Click"/>


                </StackPanel>
            </Expander>

            <Expander Header="📂 Прочее" FontSize="14" Margin="10" Padding="10">
                <StackPanel>

                    <Button Content="Типы оборудования" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="ShowTipesTO"/>
                    <Button Content="Типы деталей" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="Show_Deteil_Types"/>
                    <Button Content="Помещения" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="Show_Location"/>
                    <Button Content="Перечень работ" Margin="5" Padding="5" HorizontalAlignment="Stretch" Click="Show_Work_List"/>


                </StackPanel>
            </Expander>

            <Button Content="🗺️ Визуал поломок и ТО" Margin="10" Height="45" Padding="5" HorizontalAlignment="Stretch" Click="Show_Crash"/>
            <Button Content="📝 Отчеты" Margin="10" Height="45" Padding="5" HorizontalAlignment="Stretch" Click="Show_Reports"/>








            <Button Content="❓ Помощь" FontSize="16" Margin="10" Padding="10" Height="45" Visibility="Hidden"
                    HorizontalAlignment="Stretch" Click="ShowHelp" />
        </StackPanel>

        <!-- Основное содержание                                                                                         Главное -->
        <ContentControl x:Name="MainContent" Grid.Column="1" Visibility="Hidden">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="70" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBlock Grid.Row="0" Text="Мониторинг нарядов на ТО" VerticalAlignment="Center" Margin="10,0,0,0" FontSize="24" FontWeight="Bold" FontFamily="Yu Gothic UI"/>
                <TextBlock Text="Дата начала:" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" Margin="10,0,10,0"/>
                <DatePicker Name="StartDatePicker" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="90,0,0,0" Width="100"  Height="30" Padding="3" Style="{StaticResource MaterialDesignOutlinedDatePicker}" SelectedDateChanged="StartDatePicker_SelectedDateChanged"/>

                <TextBlock Text="Дата конца:" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="1" Margin="230,0,10,0"/>
                <DatePicker Name="EndDatePicker" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="310,0,0,0" Width="100"   Height="30" Padding="3" Style="{StaticResource MaterialDesignOutlinedDatePicker}" SelectedDateChanged="EndDatePicker_SelectedDateChanged"/>



                <DataGrid x:Name="dataGridMonitorNaryad" IsReadOnly="True" AutoGenerateColumns="False" Margin="10" Grid.Row="2" Height="900" MouseDoubleClick="dataGridMonitorNaryad_MouseDoubleClick" LoadingRow="dataGridMonitorNaryad_LoadingRow">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Дата ТО" Width="auto" Binding="{Binding Date_TO,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="Номер наряда" Width="auto" Binding="{Binding ID}" />
                        <DataGridTextColumn Header="Устройство" Width="auto" Binding="{Binding Device_Name}" />
                        <DataGridTextColumn Header="ТО" Width="auto" Binding="{Binding Types_TO_Name}" />
                        <DataGridTextColumn Header="Список работ" Width="auto" Binding="{Binding Types_TO_Work_List}" />
                        <DataGridTextColumn Header="Сотрудник" Width="auto" Binding="{Binding Users_FIO}" />
                        <DataGridTextColumn Header="Дата начала выполнения" Width="auto" Binding="{Binding Date_Start,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="Дата завершения выполнения" Width="auto" Binding="{Binding Date_End,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="Статус" Width="auto" Binding="{Binding Status}" />
                        <DataGridTextColumn Header="Запчасти" Width="auto" Binding="{Binding Sklad_Deteil_ID}" />
                        <DataGridTextColumn Header="Количество запчастей" Width="auto" Binding="{Binding Sklad_Kolich}" />
                        <DataGridTextColumn Header="Документация" Width="auto" Binding="{Binding Documentation_Name_ID}" />

                        <DataGridTextColumn Header="Комментарий" Width="auto" Binding="{Binding Comment}" />
                    </DataGrid.Columns>
                </DataGrid>
                <!--<Button x:Name="MT" Content="Мониторинг ТО" Margin="40" Grid.Row="3" HorizontalAlignment="Right" Click="MT_Click"/>-->
            </Grid>
        </ContentControl>

        <ContentControl x:Name="MonitorCrash" Grid.Column="1" Visibility="Hidden">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="70" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>
                <TextBlock Grid.Row="0" Text="Мониторинг поломок оборудования" VerticalAlignment="Center" Margin="10,0,0,0" FontSize="24" FontWeight="Bold" FontFamily="Yu Gothic UI"/>

                <Button Grid.Row="1" Content="Создать" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="CrashCreateClick"/>

                <DataGrid x:Name="dataGridMonitorCrash" IsReadOnly="True" AutoGenerateColumns="False" Margin="10" Grid.Row="2" Height="900" MouseDoubleClick="dataGridMonitorCrash_MouseDoubleClick" LoadingRow="dataGridMonitorCrash_LoadingRow">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Номер" Width="auto" Binding="{Binding ID}" />
                        <DataGridTextColumn Header="Дата/Время поломки" Width="auto" Binding="{Binding Date_Crash,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="ФИО технолога" Width="auto" Binding="{Binding FIO_Tech}" />
                        <DataGridTextColumn Header="Помещение" Width="auto" Binding="{Binding Location}" />
                        <DataGridTextColumn Header="Устройство" Width="auto" Binding="{Binding Device}" />
                        <DataGridTextColumn Header="Описание" Width="auto" Binding="{Binding Comment}" />
                        <DataGridTextColumn Header="ФИО наладчика" Width="auto" Binding="{Binding FIO_Nalad}" />
                        <DataGridTextColumn Header="Дата/Время починки" Width="auto" Binding="{Binding Date_Complete,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="Статус" Width="auto" Binding="{Binding Status}" />
                    </DataGrid.Columns>
                </DataGrid>
                <!--<Button x:Name="MT" Content="Мониторинг ТО" Margin="40" Grid.Row="3" HorizontalAlignment="Right" Click="MT_Click"/>-->
            </Grid>
        </ContentControl>














        <ContentControl x:Name="Devices" Grid.Column="1" Visibility="Hidden">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="70" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>



                <TextBlock Grid.Row="0" Text="Оборудование" VerticalAlignment="Center" Margin="10,0,0,0" FontSize="24" FontWeight="Bold" FontFamily="Yu Gothic UI"/>
                <TextBox Grid.Row="1" x:Name="SearchBoxDevices" Width="150" Height="30" HorizontalAlignment="Right" VerticalContentAlignment="Center" Margin="0,0,65,0" materialDesign:HintAssist.Hint="Поиск" Style="{StaticResource MaterialDesignFloatingHintTextBox}" TextChanged="SearchBoxDevices_TextChanged"></TextBox>


                <!-- Serch Button -->
                <Button x:Name="ClearBtnDevices" Grid.Row="1" HorizontalAlignment="Right" Width="auto" Height="30" FontSize="12" Margin="0,0,10,0" Content="✖" Click="ClearBtnDevices_Click"></Button>






                <Button Grid.Row="1" Content="Создать" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="DeviceCreateClick"/>

                <!-- Data Grid -->
                <DataGrid x:Name="dataGridDevices"  IsReadOnly="True" AutoGenerateColumns="False" Margin="10" Grid.Row="2" Height="900" MouseDoubleClick="dataGridDevices_MouseDoubleClick">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Номер" Width="auto" Binding="{Binding ID}" />
                        <DataGridTextColumn Header="Наименование оборудования" Width="auto" Binding="{Binding Name_Device}" />
                        <DataGridTextColumn Header="Тип оборудования" Width="auto" Binding="{Binding Device_Type}" />
                        <DataGridTextColumn Header="Модель" Width="auto" Binding="{Binding Model}" />
                        <DataGridTextColumn Header="Серийный номер" Width="auto" Binding="{Binding Ser_Number}" />
                        <DataGridTextColumn Header="Год выпуска" Width="auto" Binding="{Binding Year_Create_Device}" />
                        <DataGridTextColumn Header="Инв номер" Width="auto" Binding="{Binding Inventory_Number}" />
                        <DataGridTextColumn Header="Помещение" Width="auto" Binding="{Binding Location}" />
                        <DataGridTextColumn Header="Наименование бух. учет" Width="auto" Binding="{Binding Name_Buh_Uch}" />

                    </DataGrid.Columns>


                </DataGrid>
            </Grid>
        </ContentControl>
        <ContentControl x:Name="NarTO" Grid.Column="1" Visibility="Hidden">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="70" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="200" />
                    <RowDefinition Height="50" />
                    <RowDefinition Height="50" />
                    <RowDefinition Height="50" />
                </Grid.RowDefinitions>



                <TextBlock Grid.Row="0" Text="Наряды ТО" VerticalAlignment="Center" Margin="10,0,0,0" FontSize="24" FontWeight="Bold" FontFamily="Yu Gothic UI"/>
                <TextBox Grid.Row="1" x:Name="SearchBoxNarTO" Width="150" Height="30" HorizontalAlignment="Right" VerticalContentAlignment="Center" Margin="0,0,65,0" materialDesign:HintAssist.Hint="Поиск" Style="{StaticResource MaterialDesignFloatingHintTextBox}" TextChanged="SearchBoxNarTO_TextChanged"></TextBox>


                <!-- Serch Button -->
                <Button x:Name="ClearBtnNarTO" Grid.Row="1" HorizontalAlignment="Right" Width="auto" Height="30" FontSize="12" Margin="0,0,10,0" Content="✖" Click="ClearBtnNarTO_Click"></Button>






                <Button Grid.Row="1" Content="Создать" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="NarTOCreateClick"/>

                <!-- Data Grid -->
                <DataGrid x:Name="dataGridNaryad" IsReadOnly="True" AutoGenerateColumns="False" Margin="10" Grid.Row="2" Height="900" MouseDoubleClick="dataGridNaryad_MouseDoubleClick">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Номер наряда" Width="auto" Binding="{Binding ID}" />
                        <DataGridTextColumn Header="Устройство" Width="auto" Binding="{Binding Device_Name}" />
                        <DataGridTextColumn Header="ТО" Width="auto" Binding="{Binding Types_TO_Name}" />
                        <DataGridTextColumn Header="Список работ" Width="auto" Binding="{Binding Types_TO_Work_List}" />
                        <DataGridTextColumn Header="Сотрудник" Width="auto" Binding="{Binding Users_FIO}" />
                        <DataGridTextColumn Header="Дата начала выполнения" Width="auto" Binding="{Binding Date_Start,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="Дата завершения выполнения" Width="auto" Binding="{Binding Date_End,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="Статус" Width="auto" Binding="{Binding Status}" />
                        <DataGridTextColumn Header="Запчасти" Width="auto" Binding="{Binding Sklad_Deteil_ID}" />
                        <DataGridTextColumn Header="Количество запчастей" Width="auto" Binding="{Binding Sklad_Kolich}" />
                        <DataGridTextColumn Header="Документация" Width="auto" Binding="{Binding Documentation_Name_ID}" />
                        <DataGridTextColumn Header="Дата ТО" Width="auto" Binding="{Binding Date_TO,StringFormat=\{0:dd.MM.yyyy HH:mm\}}" />
                        <DataGridTextColumn Header="Комментарий" Width="auto" Binding="{Binding Comment}" />
                    </DataGrid.Columns>
                </DataGrid>



                <!-- Верхняя строка полей -->



            </Grid>
        </ContentControl>
        <ContentControl x:Name="TechObsl" Grid.Column="1" Visibility="Hidden">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="70" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                    <RowDefinition Height="Auto" />
                </Grid.RowDefinitions>



                <TextBlock Grid.Row="0" Text="Виды ТО" VerticalAlignment="Center" Margin="10,0,0,0" FontSize="24" FontWeight="Bold" FontFamily="Yu Gothic UI"/>
                <TextBox Grid.Row="1" x:Name="SearchBoxTypesTO" Width="150" Height="30" HorizontalAlignment="Right" VerticalContentAlignment="Center" Margin="0,0,65,0" materialDesign:HintAssist.Hint="Поиск" Style="{StaticResource MaterialDesignFloatingHintTextBox}" TextChanged="SearchBoxTypesTO_TextChanged"></TextBox>


                <!-- Serch Button -->
                <Button x:Name="ClearBtnTypesTO" Grid.Row="1" HorizontalAlignment="Right" Width="auto" Height="30" FontSize="12" Margin="0,0,10,0" Content="✖" Click="ClearBtnTypesTO_Click"></Button>






                <Button Grid.Row="1" Content="Создать" Margin="10,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Width="100" Click="Types_TO_Create_Click"/>

                <!-- Data Grid -->
                <DataGrid x:Name="dataGridTypesTO"  IsReadOnly="True" AutoGenerateColumns="False" Margin="10" Grid.Row="2" Height="900" MouseDoubleClick="dataGridTypesTO_MouseDoubleClick">
                    <DataGrid.Columns>
                        <DataGridTextColumn Header="Номер" Width="auto" Binding="{Binding ID}" />
                        <DataGridTextColumn Header="Тип оборудования" Width="auto" Binding="{Binding Device_Type}" />
                        <DataGridTextColumn Header="ТО" Width="auto" Binding="{Binding Name_TO}" />
                        <DataGridTextColumn Header="Перечень работ" Width="auto" Binding="{Binding Work_List}" />
                        <DataGridTextColumn Header="Расписание" Width="auto" Binding="{Binding Raspisanie}" />

                    </DataGrid.Columns>


                </DataGrid>
            </Grid>
        </ContentControl>
