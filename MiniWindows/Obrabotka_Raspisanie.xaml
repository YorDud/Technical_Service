<Window x:Class="WpfApp4.MiniWindows.Obrabotka_Raspisanie"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Обработка расписания" Height="200" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <Button Content="Обработать расписание" Click="ProcessScheduleButton_Click" Margin="0,10,0,0"/>
            <TextBox Name="ResultTextBox" Height="100" TextWrapping="Wrap" AcceptsReturn="True" IsReadOnly="True"/>
        </StackPanel>
    </Grid>
</Window>
